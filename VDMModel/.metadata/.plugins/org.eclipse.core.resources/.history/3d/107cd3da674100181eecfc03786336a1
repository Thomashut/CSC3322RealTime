class Measurement

instance variables

startReading:Encoder;
stopReading:Encoder;

startMeasure:real;
stopMeasure:real;

operations

public Measurement: Encoder * Encoder ==> Measurement
	Measurement(en1,en2) == (
		startReading := en1;
		stopReading := en2
	);

public startReading: () ==> ()

public stopReading: () ==> ()

public getMeasurement: () ==> real


end Measurement
